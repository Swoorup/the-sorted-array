{
  "name": "@sytherax/the-sorted-array",
  "author": "Swoorup Joshi",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Sorted array functionality",
  "main": "./dist/the-sorted-array.umd.js",
  "module": "./dist/the-sorted-array.es.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/sorted-array.umd.js",
      "import": "./dist/sorted-array.es.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Swoorup/the-sorted-array.git"
  },
  "bugs": {
    "url": "https://github.com/Swoorup/the-sorted-array/issues"
  },
  "homepage": "https://github.com/Swoorup/the-sorted-array/#readme",
  "scripts": {
    "tsc": "tsc",
    "dev": "vite",
    "build": "tsc && vite build && tsc -P tsconfig.dts.json",
    "test": "vitest",
    "test-ui": "vitest --ui",
    "vite-node": "vite-node",
    "main": " npm run build && node dist/main.es.js",
    "main:debug": " npm run build && node --enable-source-maps --inspect-brk dist/main.es.js"
  },
  "dependencies": {
    "fp-ts": "^2.15.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.1.3",
    "@vitest/ui": "^0.31.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.4",
    "vite": "^4.1.0",
    "vitest": "^0.31.0"
  }
}
